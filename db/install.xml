<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/quizsms/db" VERSION="20130913" COMMENT="XMLDB file for Moodle blocks/quizsms"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_quizsms" COMMENT="Default comment for block_quizsms, please edit me" NEXT="quizsms_subscriptions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="quizsms_subscriptions" COMMENT="This stores quiz sms subscriptions" PREVIOUS="block_quizsms">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="student_id"/>
        <FIELD NAME="student_id" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" PREVIOUS="id" NEXT="subscribed"/>
        <FIELD NAME="subscribed" TYPE="binary" NOTNULL="false" SEQUENCE="false" PREVIOUS="student_id"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>